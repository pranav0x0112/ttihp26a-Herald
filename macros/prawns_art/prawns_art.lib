library(prawns_art) {
  delay_model : table_lookup;
  time_unit : "1ns";
  voltage_unit : "1V";
  current_unit : "1mA";
  capacitive_load_unit(1, pf);
  leakage_power_unit : "1nW";
  pulling_resistance_unit : "1kohm";
  
  slew_lower_threshold_pct_rise : 0.3;
  slew_upper_threshold_pct_rise : 0.7;
  slew_lower_threshold_pct_fall : 0.3;
  slew_upper_threshold_pct_fall : 0.7;
  input_threshold_pct_rise : 50.0;
  input_threshold_pct_fall : 50.0;
  output_threshold_pct_rise : 50.0;
  output_threshold_pct_fall : 50.0;

  cell(PRAWNS_ART) {
    area : 1;
    is_macro_cell : true;

    pin(clk) {
      direction : input;
      clock : true;
      capacitance : 0.001;
    }

    pin(alive) {
      direction : output;
      function : "clk";
      capacitance : 0.001;
      max_capacitance : 0.1;
      
      timing() {
        related_pin : "clk";
        timing_sense : positive_unate;
        timing_type : rising_edge;
        cell_rise(scalar) {
          values("0.1");
        }
        rise_transition(scalar) {
          values("0.05");
        }
        cell_fall(scalar) {
          values("0.1");
        }
        fall_transition(scalar) {
          values("0.05");
        }
      }
    }
  }
}